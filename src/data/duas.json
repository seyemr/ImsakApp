[
  { "id": 1, "title": "Sabah Duası", "category": "Günlük", "arabic": "اللَّهُمَّ بِكَ أَصْبَحْنَا", "turkish": "Allah’ım senin adınla sabahladık." },
  { "id": 2, "title": "Akşam Duası", "category": "Günlük", "arabic": "اللَّهُمَّ بِكَ أَمْسَيْنَا", "turkish": "Allah’ım senin adınla akşamladık." },
  { "id": 3, "title": "Yatarken Dua", "category": "Günlük", "arabic": "بِاسْمِكَ اللَّهُمَّ أَمُوتُ وَأَحْيَا", "turkish": "Allah’ım senin adınla ölür ve dirilirim." },
  { "id": 4, "title": "Uyanınca Dua", "category": "Günlük", "arabic": "الْحَمْدُ لِلَّهِ الَّذِي أَحْيَانَا", "turkish": "Bizi dirilten Allah’a hamdolsun." },
  { "id": 5, "title": "Yemek Öncesi Dua", "category": "Günlük", "arabic": "بِسْمِ اللَّهِ", "turkish": "Allah’ın adıyla." },
  { "id": 6, "title": "Yemek Sonrası Dua", "category": "Günlük", "arabic": "الْحَمْدُ لِلَّهِ الَّذِي أَطْعَمَنَا", "turkish": "Bizi yediren Allah’a hamdolsun." },
  { "id": 7, "title": "Anne Baba Duası", "category": "Aile", "arabic": "رَبِّ ارْحَمْهُمَا كَمَا رَبَّيَانِي صَغِيرًا", "turkish": "Rabbim anne babama merhamet et." },
  { "id": 8, "title": "Evlilik Duası", "category": "Aile", "arabic": "رَبَّنَا هَبْ لَنَا مِنْ أَزْوَاجِنَا", "turkish": "Rabbim bize hayırlı eşler ver." },
  { "id": 9, "title": "Evlat Duası", "category": "Aile", "arabic": "رَبِّ هَبْ لِي مِنَ الصَّالِحِينَ", "turkish": "Rabbim bana salih evlatlar ver." },
  { "id": 10, "title": "Aile Huzuru Duası", "category": "Aile", "arabic": "اللَّهُمَّ أَلِّفْ بَيْنَ قُلُوبِنَا", "turkish": "Allah’ım kalplerimizi birleştir." },
  { "id": 11, "title": "Rızık Duası", "category": "Rızık", "arabic": "اللَّهُمَّ ارْزُقْنِي", "turkish": "Allah’ım bana rızık ver." },
  { "id": 12, "title": "Borçtan Kurtulma Duası", "category": "Rızık", "arabic": "اللَّهُمَّ اقْضِ عَنِّي الدَّيْنَ", "turkish": "Allah’ım borcumu ödemeyi nasip et." },
  { "id": 13, "title": "Bereket Duası", "category": "Rızık", "arabic": "اللَّهُمَّ بَارِكْ لَنَا", "turkish": "Allah’ım bize bereket ver." },
  { "id": 14, "title": "İş Bulma Duası", "category": "Rızık", "arabic": "رَبِّ إِنِّي لِمَا أَنْزَلْتَ إِلَيَّ مِنْ خَيْرٍ فَقِيرٌ", "turkish": "Rabbim her hayra muhtacım." },
  { "id": 15, "title": "Şifa Duası", "category": "Sağlık", "arabic": "اللَّهُمَّ اشْفِنِي", "turkish": "Allah’ım bana şifa ver." },
  { "id": 16, "title": "Hastaya Dua", "category": "Sağlık", "arabic": "أَذْهِبِ الْبَاسَ رَبَّ النَّاسِ", "turkish": "Ey insanların Rabbi, hastalığı gider." },
  { "id": 17, "title": "Sabır Duası", "category": "Maneviyat", "arabic": "رَبِّ اشْرَحْ لِي صَدْرِي", "turkish": "Rabbim kalbimi ferahlat." },
  { "id": 18, "title": "Hidayet Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ اهْدِنَا", "turkish": "Allah’ım bizi doğru yola ilet." },
  { "id": 19, "title": "Tevbe Duası", "category": "Maneviyat", "arabic": "رَبِّ اغْفِرْ لِي", "turkish": "Rabbim beni bağışla." },
  { "id": 20, "title": "Korunma Duası", "category": "Korunma", "arabic": "حَسْبِيَ اللَّهُ", "turkish": "Allah bana yeter." },
  { "id": 21, "title": "İlim Duası", "category": "Maneviyat", "arabic": "رَبِّ زِدْنِي عِلْمًا", "turkish": "Rabbim ilmimi artır." },
  { "id": 22, "title": "Doğru Karar Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ اخْتَرْ لِي", "turkish": "Allah’ım benim için en hayırlısını seç." },
  { "id": 23, "title": "Korkudan Korunma Duası", "category": "Korunma", "arabic": "اللَّهُمَّ أَمِّنِّي", "turkish": "Allah’ım beni emniyete al." },
  { "id": 24, "title": "Fitneden Korunma Duası", "category": "Korunma", "arabic": "اللَّهُمَّ عَافِنِي مِنَ الْفِتَنِ", "turkish": "Allah’ım fitnelerden beni koru." },
  { "id": 25, "title": "Sıkıntıdan Kurtulma Duası", "category": "Maneviyat", "arabic": "لَا إِلَٰهَ إِلَّا أَنْتَ سُبْحَانَكَ", "turkish": "Senden başka ilah yoktur, seni tenzih ederim." },
  { "id": 26, "title": "Günah Affı Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ اغْفِرْ لِي ذَنْبِي", "turkish": "Allah’ım günahlarımı bağışla." },
  { "id": 27, "title": "Kalp Huzuru Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ طَهِّرْ قَلْبِي", "turkish": "Allah’ım kalbimi temizle." },
  { "id": 28, "title": "İman Güçlendirme Duası", "category": "Maneviyat", "arabic": "رَبَّنَا لَا تُزِغْ قُلُوبَنَا", "turkish": "Rabbim kalplerimizi kaydırma." },
  { "id": 29, "title": "Hayırlı Ömür Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ بَارِكْ لِي فِي عُمْرِي", "turkish": "Allah’ım ömrüme bereket ver." },
  { "id": 30, "title": "Ahiret Selameti Duası", "category": "Maneviyat", "arabic": "رَبَّنَا آتِنَا فِي الْآخِرَةِ حَسَنَةً", "turkish": "Rabbim ahirette bize iyilik ver." },
  { "id": 31, "title": "Emanet Duası", "category": "Korunma", "arabic": "أَسْتَوْدِعُكَ اللَّهَ نَفْسِي", "turkish": "Allah’ım kendimi sana emanet ediyorum." },
  { "id": 32, "title": "Yolculuk Duası", "category": "Günlük", "arabic": "سُبْحَانَ الَّذِي سَخَّرَ لَنَا هَذَا", "turkish": "Bu aracı bize musahhar kılan Allah’a hamdolsun." },
  { "id": 33, "title": "Yağmur Duası", "category": "Günlük", "arabic": "اللَّهُمَّ صَيِّبًا نَافِعًا", "turkish": "Allah’ım faydalı yağmur gönder." },
  { "id": 34, "title": "Afetlerden Korunma Duası", "category": "Korunma", "arabic": "اللَّهُمَّ احْفَظْنَا", "turkish": "Allah’ım bizi koru." },
  { "id": 35, "title": "Hayırlı İş Duası", "category": "Rızık", "arabic": "اللَّهُمَّ وَفِّقْنِي لِلْخَيْرِ", "turkish": "Allah’ım beni hayra muvaffak kıl." },
  { "id": 36, "title": "İş Başarı Duası", "category": "Rızık", "arabic": "رَبِّ اشْرَحْ لِي صَدْرِي", "turkish": "Rabbim işimi kolaylaştır." },
  { "id": 37, "title": "Helal Kazanç Duası", "category": "Rızık", "arabic": "اللَّهُمَّ ارْزُقْنِي حَلَالًا", "turkish": "Allah’ım bana helal rızık ver." },
  { "id": 38, "title": "Kötü Alışkanlıklardan Kurtulma Duası", "category": "Korunma", "arabic": "اللَّهُمَّ اصْرِفْ عَنِّي السُّوءَ", "turkish": "Allah’ım kötülüğü benden uzaklaştır." },
  { "id": 39, "title": "Öfke Kontrol Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ أَذْهِبْ غَضَبَ قَلْبِي", "turkish": "Allah’ım kalbimdeki öfkeyi gider." },
  { "id": 40, "title": "Güzel Ahlak Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ حَسِّنْ خُلُقِي", "turkish": "Allah’ım ahlakımı güzelleştir." },
  { "id": 41, "title": "Komşu Hakkı Duası", "category": "Aile", "arabic": "اللَّهُمَّ أَحْسِنْ جِوَارَنَا", "turkish": "Allah’ım komşuluğumuzu güzel eyle." },
  { "id": 42, "title": "Çocukları Koruma Duası", "category": "Aile", "arabic": "اللَّهُمَّ احْفَظْ أَوْلَادَنَا", "turkish": "Allah’ım çocuklarımızı koru." },
  { "id": 43, "title": "Eşler Arası Sevgi Duası", "category": "Aile", "arabic": "اللَّهُمَّ اجْعَلْ بَيْنَنَا مَوَدَّةً", "turkish": "Allah’ım aramıza sevgi koy." },
  { "id": 44, "title": "Kırgınlık Giderme Duası", "category": "Aile", "arabic": "اللَّهُمَّ أَلِّفْ بَيْنَ قُلُوبِنَا", "turkish": "Allah’ım kalplerimizi uzlaştır." },
  { "id": 45, "title": "Anne Sağlığı Duası", "category": "Aile", "arabic": "اللَّهُمَّ اشْفِ أُمِّي", "turkish": "Allah’ım anneme şifa ver." },
  { "id": 46, "title": "Baba Sağlığı Duası", "category": "Aile", "arabic": "اللَّهُمَّ اشْفِ أَبِي", "turkish": "Allah’ım babama şifa ver." },
  { "id": 47, "title": "Kabir Azabından Korunma", "category": "Ahiret", "arabic": "اللَّهُمَّ أَعُوذُ بِكَ مِنْ عَذَابِ الْقَبْرِ", "turkish": "Allah’ım kabir azabından sana sığınırım." },
  { "id": 48, "title": "Cehennemden Kurtuluş Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ أَجِرْنِي مِنَ النَّارِ", "turkish": "Allah’ım beni ateşten koru." },
  { "id": 49, "title": "Cennet Talebi Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ إِنِّي أَسْأَلُكَ الْجَنَّةَ", "turkish": "Allah’ım senden cennet istiyorum." },
  { "id": 50, "title": "Hesap Kolaylığı Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ حَاسِبْنِي حِسَابًا يَسِيرًا", "turkish": "Allah’ım beni kolay hesaba çek." },
  { "id": 51, "title": "İmanla Ölüm Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ تَوَفَّنَا مُسْلِمِينَ", "turkish": "Allah’ım bizi imanla vefat ettir." },
  { "id": 52, "title": "Salihlerle Haşir Duası", "category": "Ahiret", "arabic": "وَأَلْحِقْنِي بِالصَّالِحِينَ", "turkish": "Beni salihlerle birlikte eyle." },
  { "id": 53, "title": "Peygamberimize Komşuluk Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ اجْمَعْنَا بِالنَّبِيِّ", "turkish": "Allah’ım bizi Peygamberimizle buluştur." },
  { "id": 54, "title": "Şehadetle Ölüm Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ ارْزُقْنَا الشَّهَادَةَ", "turkish": "Allah’ım bize şehitlik nasip et." },
  { "id": 55, "title": "Hayırlı Son Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ اخْتِمْ لَنَا بِالْخَيْرِ", "turkish": "Allah’ım sonumuzu hayır eyle." },
  { "id": 56, "title": "Fitneden Uzak Durma Duası", "category": "Korunma", "arabic": "اللَّهُمَّ جَنِّبْنَا الْفِتَنَ", "turkish": "Allah’ım bizi fitnelerden uzak tut." },
  { "id": 57, "title": "Şeytandan Korunma Duası", "category": "Korunma", "arabic": "أَعُوذُ بِاللَّهِ مِنَ الشَّيْطَانِ", "turkish": "Şeytandan Allah’a sığınırım." },
  { "id": 58, "title": "Göz ve Kalp Koruma Duası", "category": "Korunma", "arabic": "اللَّهُمَّ احْفَظْ بَصَرِي وَقَلْبِي", "turkish": "Allah’ım gözümü ve kalbimi koru." },
  { "id": 59, "title": "Evden Çıkarken Dua", "category": "Günlük", "arabic": "بِسْمِ اللَّهِ تَوَكَّلْتُ عَلَى اللَّهِ", "turkish": "Allah’ın adıyla, Allah’a tevekkül ettim." },
  { "id": 60, "title": "Eve Girerken Dua", "category": "Günlük", "arabic": "اللَّهُمَّ إِنِّي أَسْأَلُكَ خَيْرَ الْمَوْلِجِ", "turkish": "Allah’ım hayırlı giriş nasip et." },
  { "id": 61, "title": "Uyku Öncesi Teslimiyet Duası", "category": "Günlük", "arabic": "اللَّهُمَّ أَسْلَمْتُ نَفْسِي إِلَيْكَ", "turkish": "Allah’ım nefsimi sana teslim ettim." },
  { "id": 62, "title": "Güzel Rüya Duası", "category": "Günlük", "arabic": "اللَّهُمَّ أَرِنِي رُؤْيَا صَالِحَةً", "turkish": "Allah’ım bana hayırlı rüyalar göster." },
  { "id": 63, "title": "Kabuslardan Korunma", "category": "Korunma", "arabic": "اللَّهُمَّ احْفَظْنِي مِنَ الْكَوَابِيسِ", "turkish": "Allah’ım beni kabuslardan koru." },
  { "id": 64, "title": "Zorlukta Kolaylık Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ لَا سَهْلَ إِلَّا مَا جَعَلْتَهُ سَهْلًا", "turkish": "Allah’ım kolaylığı sen verirsin." },
  { "id": 65, "title": "Keder Giderme Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ أَذْهِبْ عَنِّي الْهَمَّ", "turkish": "Allah’ım kederimi gider." },
  { "id": 66, "title": "Kalp Sükûneti Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ أَنْزِلِ السَّكِينَةَ فِي قَلْبِي", "turkish": "Allah’ım kalbime huzur indir." },
  { "id": 67, "title": "Doğru Söz Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ اهْدِنِي لِقَوْلٍ سَدِيدٍ", "turkish": "Allah’ım bana doğru söz söylemeyi nasip et." },
  { "id": 68, "title": "Nefis Terbiyesi Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ زَكِّ نَفْسِي", "turkish": "Allah’ım nefsimi arındır." },
  { "id": 69, "title": "Şükür Duası", "category": "Maneviyat", "arabic": "رَبِّ أَوْزِعْنِي أَنْ أَشْكُرَ نِعْمَتَكَ", "turkish": "Rabbim beni şükredenlerden eyle." },
  { "id": 70, "title": "Nimet Artışı Duası", "category": "Rızık", "arabic": "اللَّهُمَّ زِدْنِي مِنْ فَضْلِكَ", "turkish": "Allah’ım lütfunu artır." },
  { "id": 71, "title": "Kazançta Helallik Duası", "category": "Rızık", "arabic": "اللَّهُمَّ بَارِكْ لِي فِي كَسْبِي", "turkish": "Allah’ım kazancımı bereketli kıl." },
  { "id": 72, "title": "İsraftan Korunma Duası", "category": "Rızık", "arabic": "اللَّهُمَّ جَنِّبْنِي الْإِسْرَافَ", "turkish": "Allah’ım beni israftan koru." },
  { "id": 73, "title": "Fakirlikten Sığınma", "category": "Rızık", "arabic": "اللَّهُمَّ إِنِّي أَعُوذُ بِكَ مِنَ الْفَقْرِ", "turkish": "Allah’ım fakirlikten sana sığınırım." },
  { "id": 74, "title": "Zenginlik Şükrü Duası", "category": "Rızık", "arabic": "اللَّهُمَّ اجْعَلْنِي مِنَ الشَّاكِرِينَ", "turkish": "Allah’ım beni şükredenlerden eyle." },
  { "id": 75, "title": "Gizli Tehlikelerden Korunma", "category": "Korunma", "arabic": "اللَّهُمَّ احْفَظْنَا مِنْ كُلِّ شَرٍّ", "turkish": "Allah’ım her türlü kötülükten bizi koru." },
  { "id": 76, "title": "Ağız ve Dil Koruma Duası", "category": "Korunma", "arabic": "اللَّهُمَّ احْفَظْ لِسَانِي", "turkish": "Allah’ım dilimi koru." },
  { "id": 77, "title": "Göz Zinasından Korunma", "category": "Korunma", "arabic": "اللَّهُمَّ غُضَّ بَصَرِي", "turkish": "Allah’ım gözümü haramdan koru." },
  { "id": 78, "title": "Kul Hakkından Korunma", "category": "Korunma", "arabic": "اللَّهُمَّ بَرِّئْ ذِمَّتِي", "turkish": "Allah’ım beni kul hakkından beri kıl." },
  { "id": 79, "title": "Zina ve Haramlardan Korunma", "category": "Korunma", "arabic": "اللَّهُمَّ اصْرِفْ عَنِّي الْفَوَاحِشَ", "turkish": "Allah’ım beni haramlardan uzaklaştır." },
  { "id": 80, "title": "Gıybetten Korunma", "category": "Korunma", "arabic": "اللَّهُمَّ احْفَظْنِي مِنَ الْغِيبَةِ", "turkish": "Allah’ım beni gıybetten koru." },
  { "id": 81, "title": "Hasedden Korunma", "category": "Korunma", "arabic": "اللَّهُمَّ طَهِّرْ قَلْبِي مِنَ الْحَسَدِ", "turkish": "Allah’ım kalbimi hasetten temizle." },
  { "id": 82, "title": "Kibirden Arınma Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ نَقِّ قَلْبِي مِنَ الْكِبْرِ", "turkish": "Allah’ım kalbimi kibirden arındır." },
  { "id": 83, "title": "Tevazu Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ اجْعَلْنِي مِنَ الْمُتَوَاضِعِينَ", "turkish": "Allah’ım beni mütevazı kullarından eyle." },
  { "id": 84, "title": "Sünnete Uyum Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ أَحْيِنِي عَلَى سُنَّةِ نَبِيِّكَ", "turkish": "Allah’ım beni Peygamberinin sünneti üzere yaşat." },
  { "id": 85, "title": "Bidatten Korunma Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ جَنِّبْنِي الْبِدَعَ", "turkish": "Allah’ım beni bidatlerden uzak tut." },
