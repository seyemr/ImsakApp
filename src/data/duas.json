  { "id": 87, "title": "Salih Amel Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ وَفِّقْنِي لِصَالِحِ الْأَعْمَالِ", "turkish": "Allah’ım beni salih amellere muvaffak kıl." },
  { "id": 88, "title": "İhlâs Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ اجْعَلْ عَمَلِي خَالِصًا", "turkish": "Allah’ım amellerimi ihlaslı eyle." },
  { "id": 89, "title": "Güzel Son Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ أَحْسِنْ عَاقِبَتَنَا", "turkish": "Allah’ım akıbetimizi hayır eyle." },
  { "id": 90, "title": "Hesap Gününde Kurtuluş Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ نَجِّنَا يَوْمَ الْحِسَابِ", "turkish": "Allah’ım hesap gününde bizi kurtar." },
  { "id": 91, "title": "Sırat Köprüsü Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ سَهِّلْ لَنَا الصِّرَاطَ", "turkish": "Allah’ım sıratı bize kolay kıl." },
  { "id": 92, "title": "Cennet Kapıları Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ افْتَحْ لَنَا أَبْوَابَ الْجَنَّةِ", "turkish": "Allah’ım cennet kapılarını bize aç." },
  { "id": 93, "title": "Peygamber Şefaati Duası", "category": "Ahiret", "arabic": "اللَّهُمَّ ارْزُقْنَا شَفَاعَةَ نَبِيِّكَ", "turkish": "Allah’ım Peygamberinin şefaatini nasip et." },
  { "id": 94, "title": "Allah Rızası Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ ارْضَ عَنَّا", "turkish": "Allah’ım bizden razı ol." },
  { "id": 95, "title": "Rahmet Duası", "category": "Maneviyat", "arabic": "اللَّهُمَّ ارْحَمْنَا", "turkish": "Allah’ım bize merhamet et." },
  { "id": 96, "title": "Ümmet İçin Dua", "category": "Maneviyat", "arabic": "اللَّهُمَّ أَصْلِحْ أُمَّةَ مُحَمَّدٍ", "turkish": "Allah’ım Muhammed ümmetini ıslah et." },
  { "id": 97, "title": "Mazlumlar İçin Dua", "category": "Maneviyat", "arabic": "اللَّهُمَّ انْصُرِ الْمَظْلُومِينَ", "turkish": "Allah’ım mazlumlara yardım et." },
  { "id": 98, "title": "Dünya ve Ahiret Hayrı Duası", "category": "Maneviyat", "arabic": "رَبَّنَا آتِنَا فِي الدُّنْيَا حَسَنَةً", "turkish": "Rabbim dünyada da ahirette de bize iyilik ver." },
